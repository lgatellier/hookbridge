{
    "tickettogitlab": {
        "auth_headers": {
            "X-Api-Token": "MySuperToken"
        },
        "input": {
            "body": {
                "$.num": {
                    "equalsTo": "12345",
                    "context_variable": "TICKET_NUM"
                }
            }
        },
        "output": {
            "gitlab": {
                "url": "https://gitlab.com/api/v4/projects/lgatellier%2Ftest/issues",
                "headers": {
                    "Private-Token": "#env[GITLAB_API_TOKEN]"
                },
                "body": {
                    "title": "Ticket #context[TICKET_NUM]"
                },
                "context_variables": {
                    "$.id": "GITLAB_ISSUE_ID"
                }
            }
        }
    }
}
